SELECT p.id, p.name, p.price, p.stock, c.name FROM products p INNER JOIN categories c on c.id=p.id
SELECT products.id, products.name, products.price, products.stock, categories.name FROM products INNER JOIN categories ON categories.id=products.id

ALTER TABLE products
ADD CONSTRAINT FK_Categories
FOREIGN KEY (category) REFERENCES categories(id);

ALTER TABLE sales
ADD CONSTRAINT FK_Products
FOREIGN KEY (product_id) REFERENCES products(id)
ON DELETE SET NULL;

ALTER TABLE trains 
DROP CONSTRAINT constraintname;

ALTER TABLE sales AUTO_INCREMENT = 1


